        "\n",
        "### ANSWER ME\n",
        "MAX_MEMORY = 100_000\n",
        "BATCH_SIZE = 1000\n",
        "LR = 0.001\n",
        "\n",
        "\n",
        "@dataclass\n",
        "class bunny:\n",
        "    location: list = field(default_factory=list)\n",
